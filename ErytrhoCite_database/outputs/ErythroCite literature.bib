	note = {Place: TIERGARTENSTRASSE 17, D-69121 HEIDELBERG, GERMANY
Publisher: SPRINGER HEIDELBERG},
	keywords = {GENOME SIZE, BODY-SIZE, RAINBOW-TROUT, SALMON ONCORHYNCHUS-NERKA, TEMPERATURE, FISH, ATLANTIC SALMON, CELL-SIZE, OXYGEN-CONSUMPTION, LARGEMOUTH BASS},
