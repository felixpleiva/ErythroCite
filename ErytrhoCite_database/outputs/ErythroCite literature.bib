	note = {Place: PO BOX 211, 1000 AE AMSTERDAM, NETHERLANDS
Publisher: ELSEVIER SCIENCE BV},
	keywords = {RAINBOW-TROUT, SALMO-GAIRDNERI, TROUT ONCORHYNCHUS-MYKISS, WINTER FLOUNDER, LIPID-PEROXIDATION, BILIARY-EXCRETION, CHRONIC TOXICITY, MALLARD DUCKLINGS, METALLOTHIONEIN INDUCTION, TISSUE DISTRIBUTION},
