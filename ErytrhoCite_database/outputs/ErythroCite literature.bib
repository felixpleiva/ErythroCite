@article{hemalatha_impact_2016,
	title = {Impact of sublethal concentration of a fungicide propiconazole on certain health biomarkers of {Indian} major carp \textit{{Labeo} rohita}},
	volume = {8},
	issn = {1878-8181},
	doi = {10.1016/j.bcab.2016.10.009},
	abstract = {The present study was conducted to determine the 96 h LC50 of propiconazole (PCZ), to the Indian major carp, Labeo rohita and to investigate hematological, biochemical, ionoregulatory and gill Na+/K+-ATPase responses to sublethal concentration of PCZ. The 96 h LC50 value of PCZ to the fish Labeo rohita was found to be 8.9 mu l L-1. Fish were exposed to 1/10th of 96 h LC50 value (0.89 mu l L-1) of PCZ for 28 days and the hematological, biochemical, ionoregulatory and gill Na+/K+-ATPase responses were evaluated at the end of 7, 14, 21 and 28 days. The results revealed a significant (p {\textless} 0.05) decrease in red blood cells (RBCs), white blood cells (WBCs), hemoglobin (Hb), hematocrit (Hct), mean corpuscular hemoglobin concentration (MCHC), plasma protein and potassium levels, while the mean corpuscular volume (MCV), mean corpuscular hemoglobin (MCH), plasma sodium, chloride and glucose levels were increased significantly (p {\textless} 0.05) as compared to the control groups. Exposure with PCZ caused a significant (p {\textless} 0.05) inhibition in gill Na+/K+-ATPase activity. These results clearly indicate that that fungicide PCZ at sublethal concentration alter the hematological, biochemical and ionoregulatory parameters of fish and alterations of these parameters can be used as biomarkers for environmental monitoring of PCZ in aquatic environment.},
	language = {English},
	journal = {Biocatalysis and Agricultural Biotechnology},
