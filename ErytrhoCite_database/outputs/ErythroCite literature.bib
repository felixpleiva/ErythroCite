	keywords = {RAINBOW-TROUT, ONCORHYNCHUS-MYKISS, BROOK TROUT, VISCOSITY, SWIMMING PERFORMANCE, BLUE MAO MAO, COHO SALMON, GAIRDNERI R, PLASMA-CORTISOL, TROUT SALVELINUS-FONTINALIS},
