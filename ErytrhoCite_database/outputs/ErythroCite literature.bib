	note = {Place: 111 RIVER ST, HOBOKEN 07030-5774, NJ USA
Publisher: WILEY},
	keywords = {ERYTHROCYTES, BODY-SIZE, RAINBOW-TROUT, SURFACE-AREA, FISH, BIOENERGETICS, BLOOD-CELL METABOLISM, MASS, OXYGEN-UPTAKE, VENTILATION FREQUENCY},
