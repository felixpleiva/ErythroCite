	note = {Place: ["Escuela de Acuicultura, Facultad de Recursos Naturales, Universidad Católica de Temuco, Casilla 15-D Temuco, Chile", "Centro de Genómica Nutricional Agroacuícola, Temuco, Chile", "Museum Alexander Koenig, Bonn, Germany"]},
	keywords = {Blood cells, Fish haematology, Galaxias maculatus, Galaxiidae, Osmeriformes, Teleostei, Whitebait},
	pages = {233--239},
	annote = {Export Date: 13 July 2024; Cited By: 3; Correspondence Address: I. Valdebenito; Escuela de Acuicultura, Facultad de Recursos Naturales, Universidad Católica de Temuco, Casilla 15-D Temuco, Chile; email: ivisler@uct.cl {\textbar} RAYYAN-INCLUSION: "Felix"={\textgreater}"Included"},
}

