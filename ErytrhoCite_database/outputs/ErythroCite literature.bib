@article{saoca_seasonal_2023,
	title = {Seasonal {Biodistribution} of {Some} {Trace} {Elements} ({Cd}, {Pb}, {Cr}, {Hg}) and "{Blood} {Biomarkers}" {Response} in \textit{{Mugil} cephalus} ({Linnaeus}, 1758)},
