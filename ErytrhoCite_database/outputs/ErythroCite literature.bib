@article{soldatov_functional_2017,
	title = {The {Functional} {Morphology} of {Erythrocytes} of the {Black} {Scorpion} {Fish} \textit{{Scorpaena} porcus} ({Linnaeus}, 1758) ({Scorpaeniformes}: {Scorpaenidae}) during {Hypoxia}},
