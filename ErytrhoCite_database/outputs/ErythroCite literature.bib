	note = {Place: 9600 GARSINGTON RD, OXFORD OX4 2DG, OXON, ENGLAND
Publisher: BLACKWELL PUBLISHING LTD},
	keywords = {TEMPERATURE, BLOOD, RED-CELLS, RESPIRATORY PROPERTIES, CHLORIDE TRANSPORT, KINETICS},
	pages = {1044--1052},
	annote = {RAYYAN-INCLUSION: "Felix"={\textgreater}"Included"},
