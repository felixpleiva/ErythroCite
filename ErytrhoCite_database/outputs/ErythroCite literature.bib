@article{silkin_hematological_2024,
	title = {The hematological parameters of the {Black} {Scorpionfish} \textit{{Scorpaena} porcus} {Linnaeus}, 1758 under {Experimental} hypothermia \textit{in vivo}},
	volume = {50},
	issn = {1063-0740},
	doi = {10.1134/S1063074024010085},
	abstract = {The linear dimensions of erythrocytes, their nuclei, and the number of cells and hemoglobin in the blood of the black scorpionfish Scorpaena porcus Linnaeus, 1758 were studied in the autumn-winter period, at water temperatures of 17, 13, 8, and 4 degrees \& Scy;. The results showed high and relatively equal hemoglobin contents and the number of erythrocytes in the blood of black scorpionfish at 17 and 13 degrees C. However, the studied parameters at a water temperature of 13 degrees \& Scy; were taken as the physiological norm, since this temperature more corresponded with the autumn temperatures of the Karadag coast. Lowering the water temperature in the aquarium to 8 degrees C reduced the hemoglobin level by 25\% and the number of erythrocytes by 26\% in the blood of S. porcus. The linear dimensions of erythrocytes did not change when the temperature dropped to 8 degrees C. At a temperature of 4 degrees C the number of erythrocytes and hemoglobin in the blood of S. porcus rapidly decreased by 57\% and by 31\%, respectively, and swelling of erythrocytes was observed. The large diameter of red blood cells increased by 11\% and the small diameter by 25\%. The linear dimensions of the nuclei of erythrocytes increased by 12\% along the major and minor axes at a temperature of 8 degrees \& Scy;. At 4 degrees C, the nuclei of erythrocytes decreased in size, along the major axis by 7\%, and along the minor axis by 9\%, indicating a disruption of the chromatin structure in the nuclear apparatus of the cell. The results we obtained made it possible to carry out a clear gradation of the temperature resistance of S. porcus. At water temperatures below 8 degrees \& Scy;, destructive processes occurred in the blood, indicating the exhaustion of protective reserves in this species. The data we obtained can be used in fish mariculture as indicators of resistance to temperature factors.},
	language = {English},
	number = {1},
	journal = {Russian Journal of Marine Biology},
	author = {Silkin, YA and Silkina, EN and Silkin, MY and Vasilets, VE},
	month = feb,
	year = {2024},
	note = {Place: 233 SPRING ST, NEW YORK, NY 10013-1578 USA
Publisher: MAIK NAUKA/INTERPERIODICA/SPRINGER},
	keywords = {ERYTHROCYTES, GROWTH, TEMPERATURE, BLOOD, AGE},
	pages = {33--42},
	annote = {RAYYAN-INCLUSION: "Felix"={\textgreater}"Included"},
}

@article{parrino_comparative_2018,
	title = {Comparative study of haematology of two teleost fish (\textit{{Mugil} cephalus} and \textit{{Carassius} auratus}) from different environments and feeding habits},
	volume = {85},
	issn = {2475-0263},
	doi = {10.1080/24750263.2018.1460694},
	abstract = {Haematological parameters are valuable indicators of fish health status. This study is aimed to provide baseline data of the blood profile of two teleost fish species living in different environments and with divergent feeding behaviour, namely the flathead grey mullet Mugil cephalus Linnaeus, 1758, a marine herbivorous fish, and the goldfish Carassius auratus (Linnaeus, 1758), a freshwater omnivorous fish. Using an automated system coupled with flow cytometry and light microscopy, significant variations were found between M. cephalus and C. auratus blood parameters, except for haemoglobin concentration (Hgb). A significant increase in red blood cell count (RBC) and haematocrit (Hct) levels, associated with reduced mean corpuscular volume (MCV), was revealed in mullets in respect to goldfish. These data may be attributable to differences in fish species, or to their divergent physiological activeness as high RBC values are associated with fast movement and high activity with streamlined bodies, or to environmental factors such as water salinity, an increase in which may lead to erythropoiesis as an adaptive process in seawater fish. Additionally, lower values of white blood cell count (WBC) and thrombocyte count (TC) were recorded in mullets with respect to goldfish, and these changes may be due to divergent feeding habits of the two fish species, or to their different environments since increased salinity may inversely affect WBC. Overall, findings from this study provide a better understanding of the influences of divergent environmental conditions and feeding habits on fish blood parameters. The combined use of an automatic haematological count with flow cytometry was demonstrated to be effective for an early assessment of blood parameters in different fish species.},
	language = {English},
	number = {1},
	journal = {European Zoological Journal},
	author = {Parrino, V and Cappello, T and Costa, G and Cannavà, C and Sanfilippo, M and Fazio, F and Fasulo, S},
	year = {2018},
	note = {Place: 2-4 PARK SQUARE, MILTON PARK, ABINGDON OR14 4RN, OXON, ENGLAND
Publisher: TAYLOR \& FRANCIS LTD},
	keywords = {MECHANISMS, FRESH-WATER, BLOOD-CHEMISTRY, CHEMISTRY VALUES, DICENTRARCHUS-LABRAX, LINNAEUS, LIZA-AURATA, MULLET, SEA, WILD},
	pages = {194--200},
